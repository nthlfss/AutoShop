@model IList<AutoShop.Models.Car>


<h1>Cars</h1>

<table class="table">

    <tr>
        <th>Year</th>
        <th>Make</th>
        <td>Model</td>
    </tr>

    @foreach (var car in Model)
    {
        <tr>
            <td>@car.Year</td>
            <td>@car.Make</td>
            <td>@car.Model</td>
        </tr>
    }
</table>

@if (Model.Count == 0)
{
    <p>No cars yet!</p>
}

<p><a asp-controller="Car" asp-action="Add">Add</a> | <a asp-controller="Car" asp-action="Remove">Remove</a></p>
